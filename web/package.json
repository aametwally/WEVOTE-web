    "config": "npm run config-client",
    "build-ec2": "npm run build-server && npm run build-metaviz && npm run build-client-ec2",
    "build": "npm run build-server && npm run build-metaviz && npm run build-client",
    "watch": "node node_modules/gulp/bin/gulp.js watch",
    "pipeline-amazon": "npm run config-ec2 && npm run build-ec2 && npm run start-ec2"
